{
  "_version": "1.59.0",
  "sap.app": {
    "id": "portalefornitori.portalefornitori",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.2",
      "toolsId": "ff336f53-51c1-4cb4-99cb-c30ee11e9f5c"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "portalefornitori.portalefornitori.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "portalefornitori.portalefornitori.view",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Master",
          "pattern": "",
          "target": ["TargetMaster", "TargetHome"]
        },
        {
          "name": "RicercaForn",
          "pattern": "cerca",
          "target": ["TargetMaster", "TargetRicercaForn"]
        },
        {
          "name": "RicercaDoc",
          "pattern": "ricerca",
          "target": ["TargetMaster", "TargetRicercaDoc"]
        },
        {
          "name": "Documenti",
          "pattern": "doc",
          "target": ["TargetMaster", "TargetDocumenti"]
        },
        {
          "name": "Template",
          "pattern": "nuovi",
          "target": ["TargetMaster", "TargetTemplate"]
        },
        {
          "name": "Anagrafica",
          "pattern": "form",
          "target": [ "TargetMaster", "TargetAnagrafica"]
        }
      ],
      "targets": {
        "TargetMaster": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "viewId": "Master",
          "viewName": "Master"
        },
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "Home",
          "viewName": "Home"
        },
        "TargetRicercaForn": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "RicercaForn",
          "viewName": "RicercaForn"
        },
        "TargetRicercaDoc": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "RicercaDoc",
          "viewName": "RicercaDoc"
        },
        "TargetDocumenti": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "Documenti",
          "viewName": "Documenti"
        },
        "TargetTemplate": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "Template",
          "viewName": "Template"
        },
        "TargetAnagrafica": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "Anagrafica",
          "viewName": "Anagrafica"
        }
      }
    },
    "rootView": {
      "viewName": "portalefornitori.portalefornitori.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
