<mvc:View controllerName="portalefornitori.portalefornitori.controller.Master"
xmlns="sap.m"
xmlns:f="sap.f"
xmlns:mvc="sap.ui.core.mvc"
xmlns:core="sap.ui.core"
xmlns:l="sap.ui.layout"
height="100%"
displayBlock="true">
<f:DynamicPage showFooter="true"
    busy="{master>/options/busy}"
    busyIndicatorDelay="0"
    backgroundDesign= "List"
    preserveHeaderStateOnScroll="false"
    toggleHeaderOnTitleClick="true">
    <f:header>
        <core:Fragment fragmentName="portalefornitori.portalefornitori.view.fragment.master.Header"
            type="XML" />
    </f:header>
    <f:content>
        <core:Fragment fragmentName="portalefornitori.portalefornitori.view.fragment.master.Routing"
            type="XML" />
    </f:content>
    <f:footer>
        <core:Fragment fragmentName="portalefornitori.portalefornitori.view.fragment.master.Footer"
            type="XML" />
    </f:footer>
</f:DynamicPage>
</mvc:View>
